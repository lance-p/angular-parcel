{
  "name": "angular-parcel",
  "version": "1.0.0",
  "description": "Boilerplate Angular application bundled with Parcel.",
  "scripts": {
    "devServe": "parcel index.html",
    "start": "npm run devServe",
    "prodBuild": "parcel build index.html -d release --public-url .",
    "prodServe": "node server.js",
    "release": "npm run prodBuild && npm run prodServe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackDeMeyers/angular-parcel.git"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "ng-bootstrap",
    "typescript",
    "parcel",
    "parcel-bundler"
  ],
  "author": "Jack DeMeyers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JackDeMeyers/angular-parcel/issues"
  },
  "homepage": "https://github.com/JackDeMeyers/angular-parcel#readme",
  "dependencies": {
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/compiler-cli": "^5.2.1",
    "@angular/core": "^5.2.1",
    "@angular/forms": "^5.2.1",
    "@angular/http": "^5.2.1",
    "@angular/platform-browser": "^5.2.1",
    "@angular/platform-browser-dynamic": "^5.2.1",
    "@angular/router": "^5.2.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.9",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "systemjs": "^0.20.19",
    "zone.js": "^0.8.20",
    "express": "^4.16.2",
    "parcel-bundler": "^1.4.1",
    "parcel-plugin-angular": "0.5.0",
    "typescript": "^2.6.2"
  }
}
