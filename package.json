{
  "name": "angular-parcel",
  "version": "1.0.0",
  "description": "Boilerplate Angular application bundled with Parcel.",
  "scripts": {
    "devServe": "parcel index.html",
    "start": "yarn run devServe",
    "prodBuild": "parcel build index.html -d release --public-url .",
    "prodServe": "node server.js",
    "release": "yarn run prodBuild && yarn run prodServe",
    "postinstall": "yarn run prodBuild",
    "herokuStart": "yarn run prodServe",
    "clean": "rm -rf .cache/ dist/ release/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackDeMeyers/angular-parcel.git"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "ng-bootstrap",
    "typescript",
    "parcel",
    "parcel-bundler"
  ],
  "author": "Jack DeMeyers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JackDeMeyers/angular-parcel/issues"
  },
  "homepage": "https://github.com/JackDeMeyers/angular-parcel#readme",
  "dependencies": {
    "@angular/common": "^5.2.4",
    "@angular/compiler": "^5.2.4",
    "@angular/compiler-cli": "^5.2.4",
    "@angular/core": "^5.2.4",
    "@angular/forms": "^5.2.4",
    "@angular/http": "^5.2.4",
    "@angular/platform-browser": "^5.2.4",
    "@angular/platform-browser-dynamic": "^5.2.4",
    "@angular/router": "^5.2.4",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "core-js": "^2.5.3",
    "express": "^4.16.2",
    "parcel-bundler": "^1.5.1",
    "parcel-plugin-angular": "^0.5.1-next.15",
    "parcel-plugin-typescript": "^0.6.0",
    "rxjs": "^5.5.6",
    "systemjs": "^0.21.0",
    "typescript": "^2.7.2",
    "zone.js": "^0.8.20"
  }
}