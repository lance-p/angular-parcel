{
  "name": "angular-parcel",
  "version": "1.0.0",
  "description": "Boilerplate Angular application bundled with Parcel.",
  "scripts": {
    "serve": "parcel index.html",
    "start": "npm run serve",
    "build": "parcel build index.html -d release --public-url .",
    "prod": "npm run build && cd release && http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackDeMeyers/angular-parcel.git"
  },
  "keywords": [
    "angular",
    "bootstrap",
    "ng-bootstrap",
    "typescript",
    "parcel",
    "parcel-bundler"
  ],
  "author": "Jack DeMeyers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JackDeMeyers/angular-parcel/issues"
  },
  "homepage": "https://github.com/JackDeMeyers/angular-parcel#readme",
  "dependencies": {
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.7",
    "core-js": "^2.5.2",
    "rxjs": "^5.5.5",
    "systemjs": "^0.20.19",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "parcel-bundler": "git+https://github.com/parcel-bundler/parcel.git",
    "parcel-plugin-typescript": "^0.2.1",
    "typescript": "^2.6.2"
  }
}
