{
  "name": "angular-parcel",
  "version": "1.0.0",
  "description": "Boilerplate Angular application bundled with Parcel.",
  "scripts": {
    "build": "tsc",
    "buildWatch": "tsc -w",
    "spin": "concurrently \"npm run buildWatch\" \"parcel app/index.html\"",
    "start": "npm run build && npm run spin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackDeMeyers/angular-parcel.git"
  },
  "keywords": [
    "angular",
    "parcel",
    "parcel-bundler"
  ],
  "author": "Jack DeMeyers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JackDeMeyers/angular-parcel/issues"
  },
  "homepage": "https://github.com/JackDeMeyers/angular-parcel#readme",
  "dependencies": {
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "core-js": "^2.5.2",
    "rxjs": "^5.5.5",
    "systemjs": "^0.20.19",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "typescript": "^2.6.2"
  }
}
